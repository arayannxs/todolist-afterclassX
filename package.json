{
  "name": "afterclass-todo",
  "version": "1.0.0",
  "main": "tailwind.config.js",
  "scripts": {
    "start": "npm-run-all --parallel watch:tailwind watch:ts",
    "watch:tailwind": "npx tailwindcss -i ./src/styles/input.css -o ./src/styles/main.css --watch",
    "watch:ts": "tsc -w",
    "build": "npm run build:tailwind && npm run build:ts",
    "build:tailwind": "npx tailwindcss -i ./src/styles/input.css -o ./src/styles/main.css --minify",
    "build:ts": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "tailwindcss": "^3.4.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "typescript": "^5.4.5"
  }
}